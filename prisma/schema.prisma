

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Product{
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  category String
  price Float
  variants Json[] 
  productId String

}

model Counter {
  id   String @id @default(auto()) @map("_id") @db.ObjectId
  name String @unique
  seq  Int
}


