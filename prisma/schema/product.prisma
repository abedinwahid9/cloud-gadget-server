model Product{
  id String  @id @default(auto()) @map("_id") @db.ObjectId 
  productId String 
  name String
  description String
  images String[]
  category String
  sub_category String 
  price Float
  discount Float
  stock_quantity Int
  status Boolean
  variants Json[] 
  reviews Json
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
